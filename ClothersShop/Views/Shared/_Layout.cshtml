<body>

    <div class="wrapper">
        <!-- ============================================================= TOP NAVIGATION ============================================================= -->
        <nav class="top-bar animate-dropdown">
            <div class="container">
                <div class="col-xs-12 col-sm-6 no-margin">
                    <ul>

                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#change-colors">Đổi Màu Web</a>
                            <ul class="dropdown-menu" role="menu">
                                <li role="presentation"><a role="menuitem" class="changecolor green-text" tabindex="-1" href="#" title="Green color">Xanh Lá</a></li>
                                <li role="presentation"><a role="menuitem" class="changecolor blue-text" tabindex="-1" href="#" title="Blue color">Xanh Da Trời</a></li>
                                <li role="presentation"><a role="menuitem" class="changecolor red-text" tabindex="-1" href="#" title="Red color">Đỏ</a></li>
                                <li role="presentation"><a role="menuitem" class="changecolor orange-text" tabindex="-1" href="#" title="Orange color">Cam</a></li>
                                <li role="presentation"><a role="menuitem" class="changecolor navy-text" tabindex="-1" href="#" title="Navy color">Xanh Da Trời Đậm</a></li>
                                <li role="presentation"><a role="menuitem" class="changecolor dark-green-text" tabindex="-1" href="#" title="Darkgreen color">Xanh Lá Đậm</a></li>
                            </ul>
                        </li>
                        <li> @{Html.RenderAction("MainMenu", "Home"); }</li>
                    </ul>
                </div><!-- /.col -->
                <div class="col-xs-12 col-sm-6 no-margin">
                    <ul class="right">
                        <li>@Html.Partial("_LoginPartial")</li>
                    </ul>
                </div><!-- /.col -->
            </div><!-- /.container -->
        </nav><!-- /.top-bar -->
        <!-- ============================================================= TOP NAVIGATION : END ============================================================= -->       <!-- ============================================================= HEADER ============================================================= -->
        <header class="no-padding-bottom header-alt">
            <div class="container no-margin">

                <div class="col-xs-12 col-md-4 logo-holder">
                    <!-- ============================================================= LOGO ============================================================= -->
                    <div class="logo">
                        <a href="/trang-chu">
                            <img alt="logo" src="/assets/client/assets/images/logo.png" width="400" height="130">
                        </a>
                    </div><!-- /.logo -->
                    <!-- ============================================================= LOGO : END ============================================================= -->
                </div><!-- /.logo-holder -->
                <div class="col-xs-12 col-md-6 top-search-holder no-margin">
                    <div class="contact-row">

                        <div class="social-icons">
                            <ul>
                                <li><a href="https://www.facebook.com/yeunhac.net" class="fa fa-facebook"></a></li>
                                <font color="blue"><big><b> Facebook </b></big></font>
                            </ul>
                        </div>
                    </div><!-- /.contact-row -->
                    <!-- ============================================================= SEARCH AREA ============================================================= -->
                    <div class="search-area">
                        <form>
                            <div class="control-group">
                                <input class="search-field" placeholder="Bạn muốn tìm?" />
                                <ul class="categories-filter animate-dropdown">
                                    <li class="dropdown">
                                        <a class="dropdown-toggle" data-toggle="dropdown" href="category-grid.html">Tất cả</a>
                                        <ul class="dropdown-menu" role="menu">
                                            <li role="presentation"><a role="menuitem" tabindex="-1" href="category-grid.html">Giày</a></li>
                                            <li role="presentation"><a role="menuitem" tabindex="-1" href="category-grid.html">Áo sơ mi</a></li>
                                            <li role="presentation"><a role="menuitem" tabindex="-1" href="category-grid.html">Áo đôi</a></li>
                                            <li role="presentation"><a role="menuitem" tabindex="-1" href="category-grid.html">Áo khoác</a></li>
                                            <li role="presentation"><a role="menuitem" tabindex="-1" href="category-grid.html">Phụ kiện</a></li>
                                        </ul>
                                    </li>
                                </ul>
                                <a class="search-button" href="#"></a>
                            </div>
                        </form>
                    </div><!-- /.search-area -->
                    <!-- ============================================================= SEARCH AREA : END ============================================================= -->
                </div><!-- /.top-search-holder -->
                <div class="col-xs-12 col-md-2 top-cart-row no-margin">
                    <div class="top-cart-row-container">
                       @{Html.RenderAction("HeaderCart", "Home"); }
                        <!-- ============================================================= SHOPPING CART DROPDOWN ============================================================= -->
                       
                    
                    </div><!-- /.top-cart-row-container -->
                    <!-- ============================================================= SHOPPING CART DROPDOWN : END ============================================================= -->
                </div><!-- /.top-cart-row -->
            </div><!-- /.container -->
            <!-- ========================================= NAVIGATION ========================================= -->
            <nav id="top-megamenu-nav" class="megamenu-vertical animate-dropdown">
                <div class="container">
                    <div class="yamm navbar">
                        <div class="navbar-header">
                            <font color="white">
                                @{ Html.RenderAction("ProductCategory", "Product");}
                            </font>
                        </div><!-- /.navbar-header -->
                    </div><!-- /.navbar -->
                </div><!-- /.container -->
            </nav><!-- /.megamenu-vertical -->
            <!-- ========================================= NAVIGATION : END ========================================= -->

            <br />
          
           


         
            <!-- ============================================================= HEADER : END ============================================================= -->      
             <div div style="background-image:url(http://www.designbolts.com/wp-content/uploads/2013/02/Grunge-Seamless-Pattern-For-Website-Background.jpg);border-radius: 20px; padding: 5px; margin:5%; margin-top:0px; margin-bottom: 0px;  border: 0px solid #FF1493; word-wrap: break-word;">
                @RenderBody()
                 @RenderSection("scripts", required: false)
            </div>                                                                                                                                                   

          
   
                                                                                                                                                                <!-- ========================================= TOP BRANDS : END ========================================= -->       <!-- ============================================================= FOOTER ============================================================= -->
</header>
    </div>
     @{Html.RenderAction("Footer", "Home"); } <!-- /#footer -->
